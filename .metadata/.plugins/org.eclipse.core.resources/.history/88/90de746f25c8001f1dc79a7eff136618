package com.ecom.orderservice.service.impl;

import org.springframework.stereotype.Service;

import com.ecom.orderservice.dto.EcomOrdersDto;
import com.ecom.orderservice.model.EcomOrders;
import com.ecom.orderservice.service.OrderService;

@Service
public class EcomOrderServiceImpl implements OrderService {

	@Override
	public String createEcomOrder(EcomOrdersDto orderDto) {
		EcomOrders orderObj = 

		return null;
	}

	private EcomOrdersDto dtoToModel(EcomOrdersDto orderDto) {
		EcomOrders.builder().orderedDate(orderDto.getOrderedDate()).customerId(orderDto.getCustomerId())
				.orderLines(orderDto.getOrderLines()).orderStatus(orderDto.getOrderStatus())
				.totalAmount(orderDto.getTotalAmount()).shippingAddress(orderDto.getShippingAddress()).build();
	}

}
